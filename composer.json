{
    "name": "brucegithub/minimal-vo",
    "description": "Minimal lib to use vo:  immutability, semantic, typed objects",
    "type": "library",
    "license": "MIT",
    "config": {
      "sort-packages": true
    },
    "require": {
      "php": "^8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
      "phpspec/prophecy-phpunit": "^2.0",
      "phpunit/phpunit": "^9",
      "roave/security-advisories": "dev-master",
      "vimeo/psalm": "^4.4"
    },
    "autoload": {
      "psr-4": {
        "MinimalVo\\": "src"
      },
      "classmap": [
        "src"
      ]
    },
    "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
    },  
    "authors": [
      {
        "name": "BruceGitHub",
        "email": "roberto.diana@gmail.com"
      }
    ]
  }
